<% include partials/land-head %>


  <div id="content">
    <div class="jumbotron">
      <div class="row">
        <div class="col-lg-12">
          <img class="img-responsive" src="/img/metro-logo-400x400.png" alt="metro logo" id="metro-logo"><h1>Rides For Rewards</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <h2>Earn Rewards On Your Daily Commute</h2>
          <hr>
        </div>
      </div>
      <div class="row" id="sign-in">
        <form action="/" method="POST">
          <div class="form-group">
            <label for="username">Email Address:</label>
            <input type="text" name="username" placeholder="">
          </div>
          <div class="form-group">
            <label for="password">Password:</label>
            <input type="password" name="password" placeholder="">
          </div>
          <div class="form-group">
            <a href="/home" class="btn btn-yellow" id="sign-in-button">Sign In</a>
            <a href="/signup" class="btn btn-red" id="sign-up-button">Sign Up</a>
          </div>
        </form>
      </div>
    </div>
  </div>
